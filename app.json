{
  "expo": {
    "name": "I Know",
    "slug": "Iknow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3f3f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "bundleIdentifier": "co.hadam.iknow",
      "icon":"./assets/icon.png",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "package": "co.hadam.iknow",
      "icon":"./assets/icon.png",
      "permissions": ["CAMERA", "RECORD_AUDIO", "READ_PHONE_STATE", "READ_CONTACTS"]
    },
    "description": "",
    "githubUrl": "https://github.com/cho-hadam/I-know"
  }
}
